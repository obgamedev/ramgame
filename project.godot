; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="ram"
run/main_scene="res://MainGame.tscn"
config/icon="res://icon.png"

[autoload]

PluginSettings="*res://addons/github-integration/scripts/PluginSettings.gd"
IconLoaderGithub="*res://addons/github-integration/scripts/IconLoaderGithub.gd"
RestHandler="*res://addons/github-integration/scenes/RestHandler.tscn"
UserData="*res://addons/github-integration/scripts/user_data.gd"

[display]

window/size/width=64
window/size/height=64
window/size/test_width=500
window/size/test_height=500
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "github-integration" )

[layer_names]

3d_physics/layer_1="ground"
3d_physics/layer_2="player"
3d_physics/layer_3="object"

[rendering]

quality/2d/use_pixel_snap=true
environment/default_environment="res://default_env.tres"
